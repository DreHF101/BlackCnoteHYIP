{
    "name": "vendor/blackcnotelab",
    "description": "BlackCnoteLab Plugin",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "Blackcnotelab\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.4",
        "nesbot/carbon": "^2.64",
        "stripe/stripe-php": "^7.72",
        "authorizenet/authorizenet": ">=1.9.3",
        "coingate/coingate-php": "^3.0",
        "mollie/mollie-api-php": "^2.48",
        "mailjet/mailjet-apiv3-php": "^1.4",
        "phpmailer/phpmailer": "^6.2",
        "sendgrid/sendgrid": "^7.6",
        "intervention/image": "^2.7",
        "messagebird/php-rest-api": "^1.20",
        "textmagic/sdk": "dev-master",
        "twilio/sdk": "^6.23",
        "btcpayserver/btcpayserver-greenfield-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}