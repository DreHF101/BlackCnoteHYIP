{
  "name": "blackcnote-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:sync\"",
    "dev:sync": "browser-sync start --config bs-config.cjs",
    "dev:watch": "npm run dev:full & npm run watch:build",
    "watch:build": "nodemon --watch src --ext ts,tsx,js,jsx,css,scss --exec \"npm run build\"",
    "build": "tsc && vite build",
    "build:watch": "vite build --watch",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:performance": "node scripts/automated-testing.js performance",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "optimize:build": "node scripts/build-optimizer.js build",
    "optimize:memory": "node scripts/memory-optimizer.js optimize",
    "monitor:memory": "node scripts/memory-optimizer.js monitor",
    "dashboard": "node scripts/development-dashboard.js",
    "testing:init": "node scripts/automated-testing.js init",
    "testing:run": "node scripts/automated-testing.js run",
    "testing:unit": "node scripts/automated-testing.js unit",
    "testing:integration": "node scripts/automated-testing.js integration",
    "testing:e2e": "node scripts/automated-testing.js e2e",
    "testing:performance": "node scripts/automated-testing.js performance",
    "dev:optimized": "npm run optimize:memory && npm run dev",
    "build:optimized": "npm run optimize:build && npm run build",
    "start:full": "npm run dashboard & npm run dev",
    "clean": "rm -rf dist node_modules/.vite logs test-reports",
    "clean:cache": "rm -rf node_modules/.vite .build-cache",
    "setup": "npm install && npm run testing:init && npm run optimize:memory",
    "setup:full": "npm run setup && npm run dev:full",
    "deploy:dev": "npm run build && npm run copy:build",
    "copy:build": "cp -r dist/* blackcnote/dist/",
    "copy:build:win": "xcopy /E /I /Y dist blackcnote\\dist",
    "health:check": "node scripts/health-check.js",
    "db:backup": "node scripts/backup-db.js",
    "db:restore": "node scripts/restore-db.js",
    "logs:clear": "node scripts/clear-logs.js",
    "performance:test": "node scripts/performance-test.js",
    "security:scan": "node scripts/security-scan.js",
    "docs:generate": "node scripts/generate-docs.js",
    "workflow:status": "node scripts/workflow-status.js",
    "debug:check": "node scripts/check-debug-conflicts.js",
    "debug:test": "npm run debug:check && npm run dev:full",
    "debug:clean": "npm run logs:clear && npm run debug:check",
    "xampp:start": "node scripts/xampp-manager.js start",
    "xampp:stop": "node scripts/xampp-manager.js stop",
    "xampp:restart": "node scripts/xampp-manager.js restart",
    "xampp:status": "node scripts/xampp-manager.js status",
    "xampp:wait": "node scripts/xampp-manager.js wait",
    "backup:create": "node scripts/backup-db.js backup",
    "backup:list": "node scripts/backup-db.js list",
    "backup:restore": "node scripts/backup-db.js restore",
    "backup:test": "node scripts/backup-db.js test",
    "backup:cleanup": "node scripts/backup-db.js cleanup",
    "notify:test": "node scripts/notification-center.js test",
    "notify:history": "node scripts/notification-center.js history",
    "notify:clear": "node scripts/notification-center.js clear",
    "notify:config": "node scripts/notification-center.js config",
    "deploy:auto": "node scripts/auto-deploy.js deploy",
    "deploy:watch": "node scripts/auto-deploy.js watch",
    "deploy:status": "node scripts/auto-deploy.js status",
    "deploy:test": "node scripts/auto-deploy.js test",
    "automation:start": "concurrently \"npm run xampp:start\" \"npm run deploy:watch\"",
    "automation:stop": "npm run xampp:stop",
    "automation:restart": "npm run xampp:restart",
    "automation:status": "npm run workflow:status && npm run deploy:status",
    "automation:backup": "npm run backup:create",
    "automation:health": "npm run health:check",
    "automation:full": "npm run automation:start && npm run automation:backup && npm run automation:health",
    "wp:setup": "powershell -ExecutionPolicy Bypass -File ./setup-wordpress.ps1",
    "dev:enhanced": "node scripts/enhanced-dev-environment.js",
    "dev:monitor": "node scripts/performance-monitor.js",
    "dev:interactive": "node scripts/enhanced-dev-environment.js interactive",
    "dev:setup": "node scripts/dev-setup.js",
    "dev:health": "node scripts/enhanced-dev-environment.js health",
    "dev:status": "node scripts/enhanced-dev-environment.js status",
    "dev:logs": "node scripts/enhanced-dev-environment.js logs",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:report": "node scripts/performance-monitor.js report",
    "performance:alerts": "node scripts/performance-monitor.js alerts",
    "performance:stats": "node scripts/performance-monitor.js stats",
    "dev:docker": "vite --host 0.0.0.0 --port 5174",
    "build:docker": "vite build --outDir ../blackcnote/wp-content/themes/blackcnote/dist",
    "dev:start": "vite --host 0.0.0.0 --port 5174",
    "dev:browsersync": "browser-sync start --config bs-config.cjs",
    "dev:both": "concurrently \"npm run dev:start\" \"npm run dev:browsersync\"",
    "dev:simple": "node dev-simple.cjs"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.20",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "glob": "^10.3.10",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "terser": "^5.24.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}